<div id="meta-details" class="meta read-only">

    <div class="data">
        <h3 class="heading">ID:</h3>
        <div class="value">
            {{ registration.isNewRecord ? 'New'|t('emaillist') : registration.id }}
        </div>
    </div>

    {% if not registration.isNewRecord %}
        <div class="data">
            <h3 class="heading">{{ 'Created at'|t('emaillist') }}:</h3>
            <div class="value">
                {{ craft.app.formatter.asDatetime(registration.dateCreated) }}
            </div>
        </div>
        <div class="data">
            <h3 class="heading">{{ 'Updated at'|t('emaillist') }}:</h3>
            <div class="value">
                {{ craft.app.formatter.asDatetime(registration.dateUpdated) }}
            </div>
        </div>
    {% endif %}

</div>
